// Merge both @notin and @delta	 
@mergedFile =
    SELECT alupmj, COUNT(alupmj) AS rowcnt
    FROM @BaseFiles
   GROUP BY alupmj  ;	
  
 
 
 // Output daily delta file  into Curated folder   
 OUTPUT @mergedFile
  TO "adl://lakehillman.azuredatalakestore.net/DATA/E1/F4074/Raw/F4074rowcnt"
USING Outputters.Csv();
 
 

